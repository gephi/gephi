msgid "DynamicClusteringCoefficientPanel.undirectedRadioButton.text"
msgstr "Ненаправленный граф"

msgid "DynamicNbNodesPanel.header.description"
msgstr "Число узлов в графе"

msgid "DynamicNbNodesPanel.header.title"
