msgid "OpenIDE-Module-Short-Description"
msgstr "Стандартная реализация инструментов"

# Painter
msgid "Painter.name"
msgstr "Заливка"

msgid "Painter.description"
msgstr "Для раскрашивания узлов используйте левый клик"

# Sizer
msgid "Sizer.name"
msgstr "Размер"

msgid "Sizer.description"
msgstr "Для изменения размера узлов используйте левый клик + вертикальное движение мыши"

# Brush
msgid "Brush.name"
msgstr "Кисть"

msgid "Brush.description"
msgstr "Для раскрашивания узлов и их ближайших соседей используйте левый клик"

msgid "DiffusionMethod.None"
msgstr "Только сам узел"

msgid "DiffusionMethod.Neighbors"
msgstr "До соседей"

msgid "DiffusionMethod.NeighborsOfNeighbors"
msgstr "До соседей соседей"

msgid "DiffusionMethod.Predecessors"
msgstr "На узлы-родителей"

msgid "DiffusionMethod.Successors"
msgstr "На узлы-потомки"

# NodePencil
msgid "NodePencil.name"
msgstr "Карандаш для рисования узлов"

msgid "NodePencil.description"
msgstr "Для создания новых узлов используйте левый клик"

# EdgePencil
msgid "EdgePencil.name"
msgstr "Карандаш для рисования рёбер"

msgid "EdgePencil.description"
msgstr "Для создания новых рёбер кликните сначала на источник, затем на получатель"

msgid "EdgePencil.status1"
msgstr "Выберите узел-источник"

msgid "EdgePencil.status2"
msgstr "Выберете узел получатель"

# ShortestPath
msgid "ShortestPath.name"
msgstr "Кратчайший путь"

msgid "ShortestPath.description"
msgstr "Для отображения кратчайшего пути между двумя узлами (если он существует) кликните на два узла."

msgid "ShortestPath.status1"
msgstr "Выберите узел-источник"

msgid "ShortestPath.status2"
msgstr "Выберете узел получатель"

msgid "ShortestPath.result"
msgstr "Найден путь из {0} ребёр"

msgid "ShortestPath.noresult"
msgstr "Между выбранными узлами путь не найден"

# HeatMap
msgid "HeatMap.name"
msgstr "Тепловая карта"

msgid "HeatMap.description"
msgstr "Используйте для раскрашивания узлов по градиенту по мере удаления от выбранного узла (с учётом веса ребер как расстояния)"

msgid "HeatMap.status.maxdistance"
msgstr "Максимальная дистанция "

# Edit
msgid "Edit.name"
msgstr "Редактирование"

msgid "Edit.description"
msgstr "Используйте для редактирования атрибутов узла"
