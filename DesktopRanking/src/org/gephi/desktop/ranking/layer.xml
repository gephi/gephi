<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE filesystem PUBLIC "-//NetBeans//DTD Filesystem 1.2//EN" "http://www.netbeans.org/dtds/filesystem-1_2.dtd">
<filesystem>
    <folder name="Actions">
        <folder name="Window">
            <file name="org-gephi-desktop-ranking-RankingAction.instance">
                <attr name="component" methodvalue="org.gephi.desktop.ranking.RankingTopComponent.findInstance"/>
                <attr name="displayName" bundlevalue="org.gephi.desktop.ranking.Bundle#CTL_RankingAction"/>
                <attr name="iconBase" stringvalue="org/gephi/desktop/ranking/resources/icon.png"/>
                <attr name="instanceCreate" methodvalue="org.openide.windows.TopComponent.openAction"/>
            </file>
        </folder>
    </folder>
    <folder name="Menu">
        <folder name="Window">
            <file name="RankingAction.shadow">
                <attr name="originalFile" stringvalue="Actions/Window/org-gephi-desktop-ranking-RankingAction.instance"/>
            </file>
        </folder>
    </folder>
    <folder name="Windows2">
        <folder name="Components">
            <file name="RankingTopComponent.settings" url="RankingTopComponentSettings.xml"/>
        </folder>
        <folder name="Modes">
            <folder name="statisticsmode">
                <file name="RankingTopComponent.wstcref" url="RankingTopComponentWstcref.xml"/>
            </folder>
        </folder>
    </folder>
</filesystem>
