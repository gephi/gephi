<?xml version="1.0" encoding="UTF-8"?>
<project xmlns="http://www.netbeans.org/ns/project/1">
    <type>org.netbeans.modules.apisupport.project</type>
    <configuration>
        <data xmlns="http://www.netbeans.org/ns/nb-module-project/3">
            <code-name-base>org.gephi.ui.components</code-name-base>
            <suite-component/>
            <module-dependencies>
                <dependency>
                    <code-name-base>org.gephi.lib.flamingo</code-name-base>
                    <build-prerequisite/>
                    <compile-dependency/>
                    <run-dependency>
                        <specification-version>1.0</specification-version>
                    </run-dependency>
                </dependency>
                <dependency>
                    <code-name-base>org.gephi.ui.utils</code-name-base>
                    <build-prerequisite/>
                    <compile-dependency/>
                    <run-dependency>
                        <specification-version>0.7</specification-version>
                    </run-dependency>
                </dependency>
                <dependency>
                    <code-name-base>org.jdesktop.swingx</code-name-base>
                    <build-prerequisite/>
                    <compile-dependency/>
                    <run-dependency>
                        <specification-version>0.7</specification-version>
                    </run-dependency>
                </dependency>
                <dependency>
                    <code-name-base>org.openide.util</code-name-base>
                    <build-prerequisite/>
                    <compile-dependency/>
                    <run-dependency>
                        <specification-version>8.6.1</specification-version>
                    </run-dependency>
                </dependency>
                <dependency>
                    <code-name-base>org.openide.windows</code-name-base>
                    <build-prerequisite/>
                    <compile-dependency/>
                    <run-dependency>
                        <specification-version>6.24.1</specification-version>
                    </run-dependency>
                </dependency>
            </module-dependencies>
            <public-packages>
                <package>com.bric.awt</package>
                <package>com.bric.swing</package>
                <package>com.connectina.swing.fontchooser</package>
                <package>net.java.dev.colorchooser</package>
                <package>net.miginfocom.layout</package>
                <package>net.miginfocom.swing</package>
                <package>org.gephi.ui.components</package>
                <package>org.gephi.ui.components.gradientslider</package>
                <package>org.gephi.ui.components.richtooltip</package>
            </public-packages>
            <class-path-extension>
                <runtime-relative-path>ext/colorchooser.jar</runtime-relative-path>
                <binary-origin>release/modules/ext/colorchooser.jar</binary-origin>
            </class-path-extension>
            <class-path-extension>
                <runtime-relative-path>ext/MigLayout-3.7.1.jar</runtime-relative-path>
                <binary-origin>release/modules/ext/MigLayout-3.7.1.jar</binary-origin>
            </class-path-extension>
            <class-path-extension>
                <runtime-relative-path>ext/FontChooser.jar</runtime-relative-path>
                <binary-origin>release/modules/ext/FontChooser.jar</binary-origin>
            </class-path-extension>
        </data>
    </configuration>
</project>
